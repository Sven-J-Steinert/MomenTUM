%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;

BeginScript 'add to drymass'
   % Helium as pressurizer stays in tank for the complete mission and is added to the dry mass
   GMAT MomenTUM.DryMass = MomenTUM.DryMass + 3.5251;
   
   % load in surface Probe payload
   GMAT MomenTUM.DryMass = MomenTUM.DryMass + 20;
EndScript;
GMAT 'set status' status = 0;
Target 'Match C3' DC {SolveMode = Solve, ExitMode = SaveAndContinue, ShowProgressWindow = true};
   Propagate 'Prop 3 days' NearEarth(MomenTUM) {MomenTUM.ElapsedDays = 3};
   Vary 'Vary.V' DC(FiniteC3sec = 2155.846907565318, {Perturbation = 0.00001, Lower = 0.0, Upper = 1e100, MaxStep = 10, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   GMAT 'set status' status = 1.0;
   BeginFiniteBurn 'BeginFiniteC3' C3Finite(MomenTUM);
   Propagate NearEarth(MomenTUM) {MomenTUM.ElapsedSecs = FiniteC3sec, StopTolerance = 1e-10};
   EndFiniteBurn 'EndFiniteC3' C3Finite(MomenTUM);
   GMAT 'set status' status = 1.1;
   Achieve 'AchieveC3' DC(MomenTUM.Earth.C3Energy = 9.144, {Tolerance = 0.000000001});
