EndTarget;  % For targeter DC

Target 'Mars Catch' DC {SolveMode = Solve, ExitMode = SaveAndContinue, ShowProgressWindow = true};
   Vary 'Vary MOI.V' DC(FiniteMOIsec = 2858.29876623814, {Perturbation = .00001, Lower = 0, Upper = 1e100, MaxStep = 10, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Propagate 'backprop' BackProp NearMars(MomenTUM) {MomenTUM.Mars.Altitude = 5000, StopTolerance = 0.001};
