   EndFiniteBurn RaisePeriFinite(MomenTUM);
   
   BeginScript 'add time n count'
      % Time spend in RaisePeriFinite Burn
      GMAT Circularizesec = MomenTUM.ElapsedSecs - measure_cache;
      
      % Sum of thrust time in this section
      GMAT CircularizesecSum = CircularizesecSum + Circularizesec;
      
      % Number of thrust arcs in this section
      GMAT CircularizeCount = CircularizeCount + 1;
   EndScript;
   Propagate 'Prop TA' NearMarsBigStep(MomenTUM) {MomenTUM.Mars.TA = 270};
