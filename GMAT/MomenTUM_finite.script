%  Target desired B-plane coordinates
Target 'Targeting Mars' DC {SolveMode = Solve, ExitMode = SaveAndContinue, ShowProgressWindow = true};
   Vary 'Vary TCM.V' DC(TCMpartV = -0.009582101018075193, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.0001, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Vary 'Vary TCM.N' DC(TCMpartN = -0.009794476125333096, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.0001, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Vary 'Vary TCM.B' DC(TCMpartB = -0.03652141484601409, {Perturbation = 0.0001, Lower = -1, Upper = 1, MaxStep = 0.0001, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   Vary 'Vary TCM.sec' DC(TCMsec = 5.349999763791194, {Perturbation = 0.1, Lower = 0.0, Upper = 6, MaxStep = 0.01, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   
   BeginScript 'Apply and norm Vec'
      % apply Thust Vector components to the Thruster
      GMAT MomenTUM.S40015EarthTCM.ThrustDirection1 = TCMpartV;
      GMAT MomenTUM.S40015EarthTCM.ThrustDirection2 = TCMpartN;
      GMAT MomenTUM.S40015EarthTCM.ThrustDirection3 = TCMpartB;
      
      % norm Thust Vector by calculating corresponding scale factor
      GMAT MomenTUM.S40015EarthTCM.ThrustScaleFactor = 1/sqrt(TCMpartV^2 + TCMpartN^2 + TCMpartB^2);
   EndScript;
   Propagate 'Prop to TCM1' DeepSpace(MomenTUM) {MomenTUM.ElapsedDays = 18, StopTolerance = 1e-05};
   GMAT 'set status' status = 2.0;
   BeginFiniteBurn 'BeginFiniteTCM' TCMFinite(MomenTUM);
   Propagate 'PropBurn' DeepSpace(MomenTUM) {MomenTUM.ElapsedSecs = TCMsec};
   EndFiniteBurn 'EndFiniteTCM' TCMFinite(MomenTUM);
   GMAT 'set status' status = 2.1;
   Propagate 'Prop 260 Days' DeepSpace(MomenTUM) {MomenTUM.ElapsedDays = 260};
   Achieve 'BdotR' DC(MomenTUM.MarsInertial.BdotR = -1450, {Tolerance = 10});
   Propagate 'Prop to Mars Periapsis' NearMarsBigStep(MomenTUM) {MomenTUM.Mars.Periapsis, MomenTUM.ElapsedDays = 50};
