Target 'Raise Peri to min' DC {SolveMode = Solve, ExitMode = SaveAndContinue, ShowProgressWindow = true};
   Vary DC(FiniteRaisePeriToMinsec = 13.13177600617785, {Perturbation = 0.0001, Lower = 0.0, Upper = 1e100, MaxStep = 1, AdditiveScaleFactor = 0.0, MultiplicativeScaleFactor = 1.0});
   GMAT 'set status' status = 5.0;
